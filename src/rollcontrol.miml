%YAML 1.2
---
include: rollcontrol.h
object: src/rollcontrol.o
init: rollcontrol_init();

senders:
  rc_send_servo:
  - [data, ROLLMessage*]

receivers:
  rc_receive_state:
  - [state, VSTEMessage*]

  rc_receive_arm:
  - [code, const char*]

  rc_raw_testrc:
  - [code, unsigned char*]
  - [len, unsigned int]
  - [timestamp, unsigned char*] 
