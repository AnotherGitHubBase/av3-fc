%YAML 1.2
---
include: arm.h
object: arm.o
init: arm_init();
final: arm_final();

senders:
  arm_send_signal:
  - [code, const char*]
  
receivers:
  arm_raw_in:
  - [buffer, unsigned char*]
  - [len, unsigned int]
  - [timestamp, unsigned char*]
  
  arm_receive_imu:
  - [data, ADISMessage*]
  
  arm_receive_gps:
  - [data, GPSMessage*]
