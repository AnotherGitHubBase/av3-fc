%YAML 1.2
---
include: rollcontrol.h
object: rollcontrol.o rollControlLibrary.o
init: rollcontrol_init();

senders:
  rc_send_servo:
  - [data, RollServoMessage*]
  
receivers:
  rc_receive_imu:
  - [data, ADISMessage*]
  
  rc_receive_arm:
  - [code, const char*]
  
  rc_raw_ld_in:
  - [code, unsigned char*]
  - [len, unsigned int]
  - [timestamp, unsigned char*]
  
  rc_raw_testrc:
  - [code, unsigned char*]
  - [len, unsigned int]
  - [timestamp, unsigned char*] 
